<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Main Page</title>
    <link type="text/css" rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css" />


    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/material")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", false)

</head>
<body>

    @RenderBody()


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.13.0/moment.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.13.0/locale/en-gb.js"></script>
    @Scripts.Render("~/scripts/datetimepicker")
    @Scripts.Render("~/scripts/angular")
    @Scripts.Render("~/scripts/angular/material")
    @Scripts.Render("~/scripts/angular/app")


    @RenderSection("scripts", required: false)


</body>
</html>

